<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 無効時 -->
    <item android:state_enabled="false" android:color="#808080"/>
    <!-- 選択時 -->
    <item android:state_selected="true" android:color="#c06900"/>
